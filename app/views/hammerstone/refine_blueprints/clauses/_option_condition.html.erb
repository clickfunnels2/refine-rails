<select multiple class="font-sans mt-1 block pl-3 pr-10 py-2 text-base border-gray-300 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm rounded-md">
  <% meta[:options].each do |option| %>
    <option
      value="<%= option[:id] %>"
      <% if input[:selected].find option[:id] %>selected<% end %>
    >
      <%= option[:display] %>
    </option>
  <% end %>
</select>
